{
  "name": "@fluentify/fluentify",
  "version": "0.0.1",
  "description": "Fluent design components based framework for Vue.js",
  "author": "Alexander Wennerstr√∏m <alexanderw0310@gmail.com>",
  "main": "dist/fluentify.common.js",
  "module": "dist/fluentify.esm.js",
  "browser": "dist/fluentify.js",
  "unpkg": "dist/fluentify.js",
  "style": "dist/fluentify.css",
  "files": [
    "/build/",
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:common && npm run build:browser && npm run build:browser:min",
    "build:common": "cross-env NODE_ENV=common webpack --config build/webpack.config.common.js --progress --hide-modules",
    "build:browser:base": "webpack --config build/webpack.config.browser.js --progress --hide-modules",
    "build:browser": "cross-env NODE_ENV=browser npm run build:browser:base",
    "build:browser:min": "cross-env NODE_ENV=production npm run build:browser:base",
    "build:dll": "webpack --progress --config build/webpack.config.dll.js",
    "lint:js": "npx eslint --ext js --ext jsx --ext vue src src/**/*.spec.js build",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.{vue,css}",
    "lint:staged": "lint-staged",
    "pretest": "npm run lint",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.config.dev.js --open --mode=development",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "npm run lint:js"
    ],
    "*.{vue,css}": [
      "npx stylefmt",
      "npm run lint:css"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "add-asset-html-webpack-plugin": "^3.1.2",
    "autoprefixer": "^9.4.3",
    "babel-core": "^6.24.0",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.2",
    "eslint": "^5.11.0",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^3.1.4",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-postcss-preprocessor": "^1.0.0",
    "karma-scss-preprocessor": "^4.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mocha-css": "^1.0.1",
    "node-sass": "^4.11.0",
    "null-loader": "^0.1.1",
    "postcss": "^7.0.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "scss-loader": "0.0.1",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5",
    "yarn-or-npm": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^2.5.21"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "!!style-loader!!css-loader!mocha-css",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
      "style-loader/lib/addStyles.js"
    ],
    "loaders": [
      "style-loader",
      {
        "loader": "css-loader",
        "options": {
          "importLoaders": 1
        }
      },
      {
        "loader": "sass-loader",
        "options": {
          "importLoaders": 1
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderYW/fluentify.git"
  },
  "bugs": {
    "url": "https://github.com/AlexanderYW/fluentify/issues"
  },
  "homepage": "https://github.com/AlexanderYW/fluentify#readme",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {},
  "keywords": [
    "vue",
    "vuejs",
    "fluent",
    "design",
    "framework",
    "components"
  ]
}
