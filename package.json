{
  "name": "fluentify",
  "version": "0.0.1",
  "description": "Fluent design components based framework for Vue.js",
  "author": "Alexander Wennerstr√∏m <alexanderw0310@gmail.com>",
  "scripts": {
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "clean": "rimraf dist",
    "coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "lint:css": "stylelint src/**/*.{vue,css}",
    "lint:js": "npx eslint --ext js --ext jsx --ext vue src src/**/*.spec.js build",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:staged": "lint-staged",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "stylelint": "^11.1.1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "browser": "dist/fluentify.js",
  "bugs": {
    "url": "https://github.com/AlexanderYW/fluentify/issues"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "!!style-loader!!css-loader!mocha-css",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
      "style-loader/lib/addStyles.js"
    ],
    "loaders": [
      "style-loader",
      {
        "loader": "css-loader",
        "options": {
          "importLoaders": 1
        }
      },
      {
        "loader": "sass-loader",
        "options": {
          "importLoaders": 1
        }
      }
    ]
  },
  "files": [
    "/build/",
    "/dist/",
    "/src/"
  ],
  "homepage": "https://github.com/AlexanderYW/fluentify#readme",
  "keywords": [
    "vue",
    "vuejs",
    "fluent",
    "design",
    "framework",
    "components"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "npm run lint:js"
    ],
    "*.{vue,css}": [
      "npx stylefmt",
      "npm run lint:css"
    ]
  },
  "main": "dist/fluentify.common.js",
  "module": "dist/fluentify.esm.js",
  "peerDependencies": {
    "vue": "^2.5.21"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderYW/fluentify.git"
  },
  "style": "dist/fluentify.css",
  "unpkg": "dist/fluentify.js"
}
